{% extends 'base.html.twig' %}

{% block title %}Manager{% endblock %}

{% block body %}
    <h1>Manager</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ manager.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ manager.name }}</td>
            </tr>
            <tr>
                <th>Club</th>
                <td>{{ manager.club }}</td>
            </tr>
            <tr>
                <th>Salary</th>
                <td>{{ manager.salary }}</td>
            </tr>
            <tr>
                <th>License_date</th>
                <td>{{ manager.licenseDate ? manager.licenseDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ manager.active }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ manager.createdAt ? manager.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ manager.updatedAt ? manager.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('manager_index') }}">back to list</a>
    |
    <a href="{{ path('manager_edit', {'id': manager.id}) }}">edit</a>

    {{ include('manager/_delete_form.html.twig') }}
{% endblock %}
